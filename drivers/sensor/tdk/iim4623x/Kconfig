config IIM4623X
	bool "TDK Invensense IIM4623x IMU sensor family"
	default y
	depends on DT_HAS_INVENSENSE_IIM46234_ENABLED || \
		   DT_HAS_INVENSENSE_IIM46230_ENABLED
	select SPI
	select SPI_RTIO
	help
	  Enable TDK Invensense IIM46234 IMU sensor family support.
